<li id='waittime-<%= waittime['id'] %>' class='waittime-item'>
  <div>
    <pre></pre>
    <span><%= customer[:name] %></span>
  </div>
  <div>
    <span>Wait Time</span>
  </div>
  <div>
    <%# Trigger an SMS message.%>
    <button value='<%= customer[:phone] %>' class='table-ready-btn'>Table Almost Ready!</button>
    <%# Update Waittime object. Remove waittime-item from list. %>
    <form class='waittime-seat-form' action='/waittimes/<%= waittime['id'] %>'>
      <button class='seat-party-btn'type='submit' type="submit">Seat this Party</button>
    </form>
  </div>
</li>
